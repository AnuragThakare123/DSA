public class Main{
    
    public static void main(String args[]){
        
       int n=5;
        for(int i =1; i<=n;i++){
            
           for(char ch ='A'; ch <='A'+n-i ; ch++){
               
               System.out.print(ch+" ");
           }
           
           System.out.println();
        }
    }
}

