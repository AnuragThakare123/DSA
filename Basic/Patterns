public class Main{
    
    public static void main(String args[]){
        
        int n =3;
        
        
        for(int m=1; m<=n;m++){
             // uperwala
            for(int k=1; k<=n-m;k++){
                
                System.out.print(" "+" ");
            }
            
            for(int j=1; j<= 2*m-1; j++){
                
                System.out.print("*"+" ");
            }
            
             System.out.println();
       
        }
       
        for(int i=1; i<=n; i++){
            
        
          // Neeche wala triangle
            for(int k =1 ;k<=i-1;k++){
                
                System.out.print(" "+" ");
        
            }
            
            for(int j=1; j<= 2*n-(2*i-1);j++){
                System.out.print("*"+" ");
                
            } 
            System.out.println();
        }
    }
}
