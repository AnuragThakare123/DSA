// Brute force

import java.util.ArrayList;
public class Main
{
	public static void main(String[] args) {
   
     int n = 7;
     int count = 0;

     for(int i= 1; i<=n; i++){
         
     if(n%i == 0){
         
         count ++;
     }
   
    }
    
    if(count == 2){  
        System.out.println("num is prime");
     }
     else{
         
        System.out.println("num is not prime");

     }

	}
}
