public class Main{
    
    public static void main(String args[]){
        
        int n=4;
        char ch ='A';
        for(int i=1; i<=n; i++){
            
            for(int j = 1; j<=i; j++){
                
                System.out.print((char)(ch+j-1)+" ");
            }
            
            System.out.println();
            
        }
    }
}
