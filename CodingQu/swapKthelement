
public class Main
{
   public static void print(int arr[],int n){
       for(int i=0; i<n; i++){
           System.out.print(arr[i]+" ");
       }
   }


	public static void main(String[] args) {

    //  int arr[] = {1,2,3,4,5,6,7};
        int arr[] = {5,3,6,1,2};

     int temp;
     int k=2;
     int n =arr.length;
     //print(arr,n);
     
     temp = arr[k-1];
     arr[k-1] = arr[n-k];
     arr[n-k] = temp;
     print(arr,n);

	}
}
