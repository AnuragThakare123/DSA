public class Main{
    
    public static void main(String args[]){
        
       int n=4;
        for(int i =1; i<=n;i++){
            
           for(int j =1;  j <= n-i ; j++){
               

               System.out.print( " "+" ");
           }
           
            for(char ch  ='A';  ch <= 'A'+ i-1; ch++){
               

               System.out.print( ch +" ");
           }
           
           for(char ch  =(char)('A'+i-2);  ch >= 'A'; ch--){
               

               System.out.print( ch +" ");
           }
           
           System.out.println();
        }
    }
}
