public class Main{
    
    public static void main(String args[]){
        
       int n=4;
        for(int i =1; i<=n;i++){
            
           for(char ch ='A'; ch <= 'A'+i-1; ch++){
               
               System.out.print(ch+" ");
           }
           
           System.out.println();
        }
    }
}
