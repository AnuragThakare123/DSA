public class Main{
    
    public static void main(String args[]){
        
        for(int i =1 ; i<=5 ; i++){
            
            for(int j = 5 ;j>=i;j--){
                System.out.print(5-j+1);   
            } 
            System.out.println();
        }
    }
}
