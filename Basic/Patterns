public class Main{
    
    public static void main(String args[]){
        
        for(int i =0 ; i<5 ; i++){ // this loop always print no. of rows
            
            for(int j =0;j<5;j++){ // this loop always print no. of columns
                System.out.print("*");
            }
            
            System.out.println();
        }
    }
}
