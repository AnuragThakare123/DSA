public class Main
{
	public static void main(String[] args) {

    String str[] = {"I am Anurag"};
    int count = 0;
    int n = str.length;
    
    for(int i =0; i< n; i++){
        
        if(str[i] != " "){
            str[count] = str[i];
            count++;
        }
      }
     
     str[count] = '\0';
	}
}
